  "name": "ChatBoT",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "keywords": [],
  "author": "Rui Reogo (ruingl)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.11.0",
    "chalk": "^4.1.2",
    "cheerio": "^0.22.0",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "https-proxy-agent": "^4.0.0",
    "mqtt": "^3.0.0",
    "npmlog": "^1.2.0",
    "path": "^0.12.7",
    "request": "^2.53.0",
    "websocket-stream": "^5.5.0"
  },
  "engines": {
    "node": ">=10.x"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "mocha": "^7.0.1",
    "prettier": "^1.11.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "es2017": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    }
  }
}
